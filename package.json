{
	"name": "skc-site",
	"version": "1.7.7",
	"description": "Website to help duelists.",
	"main": "index.js",
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@emotion/react": "11.10.5",
		"@emotion/styled": "11.10.5",
		"@mui/icons-material": "5.11.0",
		"@mui/material": "5.11.8",
		"@mui/styled-engine-sc": "5.11.0",
		"@nivo/pie": "0.80.0",
		"axios": "1.3.3",
		"he": "1.2.0",
		"lodash.startcase": "4.4.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-helmet": "6.1.0",
		"react-markdown": "8.0.5",
		"react-router-dom": "6.8.1",
		"react-visibility-sensor": "5.1.1",
		"styled-components": "5.3.6"
	},
	"resolutions": {
		"d3-color": "3.1.0",
		"async": "3.2.4",
		"glob-parent": "6.0.2",
		"node-forge": "1.3.1",
		"nth-check": "2.1.1"
	},
	"scripts": {
		"preinstall": "npx npm-force-resolutions",
		"fresh-install": "rm -rf node_modules; npm install --no-optional && npx browserslist@latest --update-db",
		"start": "webpack-dev-server --mode development --env dev",
		"start:dashboard": "webpack-dashboard -- webpack serve --mode development --env dev",
		"build": "webpack --mode production --env dev",
		"build:development_remote": "webpack --mode production --env remote-dev",
		"build:production": "webpack --mode production --env prod",
		"test": "react-scripts test --coverage --watchAll",
		"ci:test": "react-scripts test --ci --coverage --watchAll=false",
		"mutation": "stryker run config/stryker.conf.json",
		"checkAWS:cloudfrontSKC": "aws cloudfront list-distributions | jq '.DistributionList.Items[] | select(.Aliases.Items | contains([\"thesupremekingscastle.com\"])) |  {id: .Id, aliases: .Aliases}'",
		"uploadAWS:production": "npm run build:production; aws s3 rm s3://yugiohsite/ --recursive; aws s3 sync build s3://yugiohsite/ --cache-control max-age=3600; aws cloudfront create-invalidation --distribution-id ${YGO_CLOUDFRONT_PROD} --paths '/*'",
		"uploadAWS:development": "npm run build:development_remote; aws s3 rm s3://yugiohsitedev/ --recursive; aws s3 sync build s3://yugiohsitedev/ --cache-control max-age=3600; aws cloudfront create-invalidation --distribution-id ${YGO_CLOUDFRONT_DEV} --paths '/*'"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"@babel/preset-env": "7.20.2",
		"@babel/preset-react": "7.18.6",
		"@babel/preset-typescript": "7.18.6",
		"@stryker-mutator/core": "6.3.1",
		"@stryker-mutator/jest-runner": "6.3.1",
		"@stryker-mutator/typescript-checker": "6.3.1",
		"@testing-library/jest-dom": "5.16.5",
		"@testing-library/react": "13.4.0",
		"@types/jest": "^29.2.4",
		"@types/lodash.startcase": "4.4.7",
		"@types/react": "18.0.28",
		"@types/react-dom": "18.0.10",
		"@types/react-helmet": "6.1.6",
		"@types/react-router-dom": "5.3.3",
		"@types/styled-components": "5.1.26",
		"@types/webpack-dev-server": "4.7.2",
		"babel-loader": "9.1.2",
		"clean-webpack-plugin": "4.0.0",
		"copy-webpack-plugin": "11.0.0",
		"eslint-config-react-app": "7.0.1",
		"html-webpack-plugin": "5.5.0",
		"jest": "^29.3.1",
		"jest-environment-jsdom": "29.4.2",
		"postcss-flexbugs-fixes": "5.0.2",
		"postcss-normalize": "10.0.1",
		"react-scripts": "5.0.1",
		"react-test-renderer": "18.2.0",
		"ts-jest": "^29.0.3",
		"ts-loader": "9.4.2",
		"ts-node": "10.9.1",
		"typescript": "4.9.5",
		"webpack": "5.75.0",
		"webpack-cli": "5.0.1",
		"webpack-dashboard": "3.3.7",
		"webpack-dev-server": "4.11.1"
	},
	"jest": {
		"coverageThreshold": {
			"global": {
				"branches": 80,
				"functions": 80,
				"lines": 80,
				"statements": 80
			}
		},
		"transformIgnorePatterns": [
			"node_modules/(?!axios)"
		],
		"coveragePathIgnorePatterns": [
			"src/components/",
			"src/helper/reducers/",
			"src/helper/DownstreamServices.ts"
		]
	}
}

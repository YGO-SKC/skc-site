{
  "name": "skc-site",
  "version": "1.3.3",
  "description": "Website to help duelists.",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mui/icons-material": "^5.2.4",
    "@mui/material": "^5.1.0",
    "@mui/styled-engine-sc": "^5.1.0",
    "@mui/styles": "^5.1.0",
    "@nivo/pie": "^0.79.0",
    "axios": "0.25.0",
    "d3-color": "3.0.1",
    "he": "1.2.0",
    "lodash.startcase": "^4.4.0",
    "nth-check": "2.0.1",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-markdown": "^8.0.0",
    "react-router": "6.2.1",
    "react-router-dom": "6.2.1",
    "react-visibility-sensor": "5.1.1",
    "set-value": "^4.1.0",
    "styled-components": "^5.3.3",
    "underscore": "1.13.2"
  },
  "scripts": {
    "start": "env-cmd -e dev react-scripts start",
    "start:development_remote": "env-cmd -e remote-dev react-scripts start",
    "start:production": "env-cmd -e prod react scripts start",
    "build": "env-cmd -e dev react-scripts build",
    "build:development_remote": "env-cmd -e remote-dev react-scripts build",
    "build:production": "env-cmd -e prod react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "checkAWS:cloudfrontSKC": "aws cloudfront list-distributions | jq '.DistributionList.Items[] | select(.Aliases.Items | contains([\"thesupremekingscastle.com\"])) |  {id: .Id, aliases: .Aliases}'",
    "uploadAWS:production": "npm run build:production; aws s3 rm s3://yugiohsite/ --recursive; aws s3 sync build s3://yugiohsite/ --cache-control max-age=3600; aws cloudfront create-invalidation --distribution-id ${YGO_CLOUDFRONT_PROD} --paths '/*'",
    "uploadAWS:development": "npm run build:development_remote; aws s3 rm s3://yugiohsitedev/ --recursive; aws s3 sync build s3://yugiohsitedev/ --cache-control max-age=3600; aws cloudfront create-invalidation --distribution-id ${YGO_CLOUDFRONT_DEV} --paths '/*'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/lodash.startcase": "4.4.6",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/react-helmet": "6.1.5",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.21",
    "@types/underscore": "1.11.4",
    "env-cmd": "10.1.0",
    "eslint-config-react-app": "7.0.0",
    "immer": "9.0.12",
    "react-scripts": "5.0.0",
    "typescript": "4.5.5"
  }
}
